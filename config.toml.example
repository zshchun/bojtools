cache_dir = "~/boj/cache"
code_dir = "~/boj/code"
template = "~/.boj/template.cpp"
open_in_browser = true
browser = "google-chrome"
pager = "less"
lang_id = 84
tab_width = 4
text_with = 70
auto_generate = true
# open, close, onlyaccepted
code_open = "open"
bojautologin = ""
solved_token = ""

[[lang]]
compile = ["g++", "-std=c++11", "-O2", "-o", "%PROB_NUM%", "%SOURCE%"]
cmd = ["./%PROB_NUM%"]
ext = "cpp"

[[lang]]
compile = []
cmd = ["python3", "%SOURCE%"]
ext = "py"
